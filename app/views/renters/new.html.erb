New Renter Form 

<br>
<br>

<%= form_for @renter do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>
<br>
<br>
<%= f.label :age %>
<%= f.number_field :age %>
<br>
<br>
<%= f.label :username %>
<%= f.text_field :username %>
<br>
<br>
<%= f.label :user_type %>
<%= f.text_field :user_type %>
<br>
<br>
<%= f.label :password %>
<%= f.password_field :password %>
<br>
<br>
<%= f.submit "submit" %>
<% end %>

<br>

<%= button_to "Back to Renters", renters_path, method: :get %>
<br>
<%= button_to "Homepage", homepages_path, method: :get %>