sign up 

<br>
<br>

 <% if flash[:errors] %>
  <% flash[:errors].each do |error| %>
    <p> <%= error %></p>
  <% end %>
<% end %>

<%= form_for @renter do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>
<br>
<br>
<%= f.label :age %>
<%= f.number_field :age %>
<br>
<br>
<%= f.label :username %>
<%= f.text_field :username %>
<br>
<br>
<%= f.label :password %>
<%= f.password_field :password %>
<br>
<br>
<%= f.submit "register" %>
<% end %>

<br>

<%= button_to "homepage", homepages_path, method: :get %>